{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "No-console":2,                   
        "comma-dangle": 1,               
        "Quotes":2,
        "Linebreak-style":0,    
        "no-unused-vars": 0,             
        "max-len": 0   
    }
}
